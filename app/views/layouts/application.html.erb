<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render "layouts/header" %>
  <%= render "layouts/notice" %>
  <div id="content">
    <%= content_for?(:content) ? yield(:content) : yield %>
  </div>
</body>
</html>
